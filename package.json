{
  "name": "athlon-quest-server",
  "version": "1.0.0",
  "main": "src/main.js",
  "type": "module",
  "scripts": {
    "start": "node src/main.js",
    "start:dev": "nodemon",
    "build": "tsc",
    "lint": "eslint src/**/*.ts",
    "db:seed": "node src/console/seed.js",
    "test": "jest"
  },
  "keywords": [
    "LMS",
    "Learning Management System",
    "Education",
    "Athlons",
    "ACA",
    "Node.js",
    "MongoDB",
    "PostgreSQL",
    "Redis",
    "Real-time Chat",
    "Assignment Submission",
    "Leaderboard",
    "Video Progress Tracking",
    "Cloudinary",
    "PDF Certificates",
    "Student Learning",
    "Facilitator Tools",
    "Admin Panel"
  ],
  "author": "Favour Max-Oti",
  "license": "ISC",
  "description": "AthlonQuest is a comprehensive learning management system designed for ACA students, known as Athlons. It facilitates seamless course management, video progress tracking, real-time chat forums, assignment submissions, and leaderboard functionality. Built with Node.js, PostgreSQL, MongoDB, and Redis, AthlonLearn empowers students, facilitators, and administrators with a robust platform for modern education.",
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/plugin-transform-runtime": "^7.25.4",
    "@babel/preset-env": "^7.25.4",
    "@faker-js/faker": "^9.0.1",
    "babel-jest": "^29.7.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.4",
    "supertest": "^7.0.0"
  },
  "jest": {
    "transform": {
      "^.+\\.js?$": "babel-jest"
    },
    "testEnvironment": "node",
    "collectCoverage": false,
    "coverageDirectory": "./coverage",
    "coverageReporters": ["text", "lcov"]
  },
  "dependencies": {
    "@types/jest": "^29.5.13",
    "argon2": "^0.41.1",
    "awilix": "^11.0.0",
    "cloudinary": "^2.5.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.0",
    "express-list-routes": "^1.2.2",
    "express-rate-limit": "^7.4.0",
    "express-session": "^1.18.0",
    "helmet": "^7.1.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
